{
    "editor.inlayHints.enabled": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "commentbars.quickPresets": [
        {
            "label": "Normal",
            "fillChar": "-",
            "width": 90,
            "thickness": 1
        }
    ],
    "commentbars.commentDelimsUser": {
        "rust": {
            "start": "// * ",
            "end": " * //",
        }
    },
    "rust-analyzer.check.command": "clippy",
    "rust-analyzer.rustc.source": "discover",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "codeium.enableConfig": {
        "*": true,
        "todo": true
    },
    "todo.file.defaultContent": "Todo:  [ ] Item",
    "todo.symbols.box": "[ ]",
    "window.titleBarStyle": "custom",
    "files.associations": {
        "*.gdextension": "toml"
    },
    "editor.minimap.enabled": false,
    "terminal.integrated.fontFamily": "\"CommitMono Nerd Font Mono\"",
    "editor.fontFamily": "\"CommitMono Nerd Font Mono\"",
    "base16.generator.activatedThemes": [
        "base16-darktooth-dark"
    ],
    "editor.formatOnSave": true,
    "editor.fontSize": 21,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "GapStyle VS",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "git.openRepositoryInParentFolders": "never",
    "C_Cpp.clang_format_fallbackStyle": "LLVM",
    "security.workspace.trust.enabled": false,
    // "platformio-ide.useBuiltinPIOCore": false,
}